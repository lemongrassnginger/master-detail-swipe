<mvc:View id="Master"
	controllerName="mud.4ever.controller.Master"
	displayBlock="true"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">

	<List
		id="master-list"
		headerText=""
		mode="SingleSelectMaster"
		select="onMasterSelect"
		items="{pages>/pages}" >
		<items>
			<ObjectListItem
				title="{pages>title}" />
			</items>
		</List>

	</mvc:View>
