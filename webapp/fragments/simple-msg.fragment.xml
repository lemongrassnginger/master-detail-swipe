<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core" >
	<Page
		title="{title}" >
		<FlexBox
			fitContainer="true"
			justifyContent="Center"
			alignItems="Center" >
			<items>
				<Title
					text="{text}"  />
				</items>
			</FlexBox>
		</Page>
	</core:FragmentDefinition>
